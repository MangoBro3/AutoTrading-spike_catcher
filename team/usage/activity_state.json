{
  "project": "openclaw-news-workspace",
  "version": "timeline-rule-v1",
  "updated_by": "pm",
  "log_file": "team/usage/activity_log.jsonl",
  "rules": {
    "events": [
      "task_started",
      "task_update",
      "blocker_set",
      "blocker_cleared",
      "test_pass",
      "test_fail",
      "task_done"
    ],
    "required_keys": [
      "ts",
      "time",
      "project",
      "agent",
      "event",
      "task_id",
      "detail"
    ],
    "debounce": {
      "window_minutes": 5,
      "key": "task_id"
    },
    "dashboard": {
      "sort": "desc",
      "page_size": 30,
      "time_format": "ko-KR"
    },
    "notes": [
      "로그/스택트레이스 원문 덤프 금지",
      "detail은 1줄 요약"
    ]
  },
  "last_event": {
    "event": "task_update",
    "task_id": "T-001"
  }
}
